<?xml version="1.0" encoding="utf-8"?>
<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="24dp"
    android:height="24dp"
    android:viewportWidth="24"
    android:viewportHeight="24">

    <group android:name="fill_group">
        <!-- fill should point to ?attr/fillColor when fully implemented -->
        <path
            android:name="fill_stroke"
            android:strokeColor="#000000"
            android:strokeLineJoin="round"
            android:strokeWidth="3.25"
            android:pathData="@string/battery_circle_path" />
    </group>

    <!-- fill should point to ?attr/backgroundColor when fully implemented -->
    <path
        android:name="frame"
        android:strokeColor="#4DFFFFFF"
        android:strokeLineJoin="round"
        android:strokeWidth="3.25"
        android:pathData="@string/battery_circle_path" />

</vector>